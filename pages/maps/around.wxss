page {
	height: 100%;
}
.container {
	height: 100%;
}


.place{
	display: inline-block;
	width: 100%;
	height: 200rpx;
}
.place-container{
	font-size: 14px;
	height: 100%;
	width: 100%;
}
.place-logo{
	float: left;
	width: 200rpx;
	height: 100%;
	background-size: cover;
}
.place-detail{
	height:100%;
	overflow: hidden;
	padding-left: 30rpx;
}
.place-name{
	color:#313131;
	font-size: 16px;
	margin-bottom: 15rpx;
	line-height: 30px;
}
.place-data{
	color:#707070;
}
.place_name{
	float:left;
	margin-right: 50rpx;
}
.place_addr{
	float: left;
}
.place_tel{
	float: right;
	margin-right: 10rpx;
}

page{
	font-family: "Microsoft Yahei, Arial, Helvetica, sans-serif";
	font-size: 12px;
}
