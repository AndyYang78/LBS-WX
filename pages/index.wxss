page {
	height: 100%;
}
.place-container{
	
}


.page_list{
	display: inline-block;
	width: 100%;
	height: 150rpx;
}

.place-logo{
	float: left;
	width: 200rpx;
	height: 100%;
	background-size: cover;
}
.place-detail{
	height:100%;
	overflow: hidden;
	padding-left: 30rpx;
}
.place-name{
	color:#313131;
	font-size: 16px;
	margin-bottom: 15rpx;
	line-height: 30px;
}
.place-data{
	color:#707070;
    font-size: 14px;
}
.place_name{
	float:left;
	margin-right: 50rpx;
}
.place_addr{
	float: left;
}
.place_tel{
	float: right;
	margin-right: 10rpx;
}

page{
	font-family: "Microsoft Yahei, Arial, Helvetica, sans-serif";
	font-size: 12px;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.userinfo-avatar {
  width: 48rpx;
  height: 48rpx;
  margin: 10rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #fff;
}



 